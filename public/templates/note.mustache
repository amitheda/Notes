<div id="noteContainer">
	<fieldset class = "fieldset">
    <legend>Note</legend>
    <table class="tbl">
    <tr>
      <td><div id="error" >{{error}}</div></td>       
    </tr>
    <tr>
      <td><div class="title">{{title}}</div></td> 
    </tr>
    <tr>
      <td><div class="body">{{body}}</div></td> 
    </tr>
    {{#noteTags}}
    <tr>
      <td><span class="label">{{tag}}</span>
      </td>
    </tr>
    {{/noteTags}}
    <tr>
      <td><div><a href="/notes/create">Update</a></div></td>
      <td><div><a href="/notes"><button type="button">Back</button></a></div></td>
    </tr>
    </table>
    </fieldset>
</div>